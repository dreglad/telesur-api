type Article {
  id: ID! @unique
  url: String! @unique
  headline: String!
  description: String
  datePublished: DateTime!
  body: String!
  author: String
  tags: [String!]
  images: [String!]
  sections: [ArticleSection!]!
  service: Service!
}

type ArticleSection {
  id: ID! @unique
  name: String! @unique
  articles: [Article!]!
}
