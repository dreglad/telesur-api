type Article {
  id: ID! @unique
  url: String! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  headline: String!
  description: String
  datePublished: DateTime!
  dateModified: DateTime
  body: String!
  author: String
  tags: [String!]
  images: [String!]
  sections: [ArticleSection!]!
  service: Service!
}

type ArticleSection {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  name: String! @unique
  articles: [Article!]!
}
