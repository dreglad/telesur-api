type Service {
  id: ID! @unique
  name: ID! @unique
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  language: LANGUAGE
  url: String
  liveUrl: String
  videoUrl: String
  videoRestUrl: String
  articles: [Article!]!
  users: [User!]!
  videos: [Video!]!
}

type User {
  id: ID! @unique
  email: String! @unique
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  name: String
  services: [Service!]!
  playlists: [Playlist!]!
}

enum LANGUAGE {
  ES
  EN
}
